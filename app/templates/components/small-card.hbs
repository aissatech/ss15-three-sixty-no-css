<div class="inner">
  <div class="img"><img {{bind-attr src=data.owner.avatar_url}} /></div>
  <div class="name"><a {{bind-attr href=data.html_url}} target="_blank">{{data.name}}</a></div>
  <div class="stats">

    <div class="stats-atk">{{data.atk}}</div>
    <span>/</span>
    <div class="stats-def">{{data.def}}</div>
  </div>
</div>
